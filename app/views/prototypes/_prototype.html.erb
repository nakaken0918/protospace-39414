<div class="card">
  <%= link_to image_tag(prototype.user.image, class: :card__img ), root_path%>
  <div class="card__body">
    <%= link_to prototype.user.title, root_path, class: :card__title%>
    <p class="card__summary">
      <%= prototype.user.catch_copy %>
    </p>
    <%= link_to prototype.user.name, root_path, class: :card__user %>
  </div>
</div>